    "16": "icons/janus-16.png",
    "32": "icons/janus-32.png",
    "48": "icons/janus-48.png",
    "64": "icons/janus-64.png",
    "96": "icons/janus-96.png",
    "128": "icons/janus-128.png"
  },
  "manifest_version": 3,
  "name": "Janus",
  "permissions": [
    "tabs"
  ],
  "version": "0.0.1"
}
